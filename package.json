{
  "name": "githubactions",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "dependencies": {
    "expo": "~45.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/react-native": "^11.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.17",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*-(test|spec).(js|ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/ios",
      "<rootDir>/android",
      "<rootDir>/node_modules"
    ]
  }
}
